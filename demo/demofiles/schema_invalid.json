{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "exampleKey1": {
      "type": "string",
      "maxLength": 15
    },
    "exampleKey2": {
      "type": "integer",
      "minimum": -100,
      "maximum": 100
    },
    "exampleObject": {
      "type": "object",
      "patternProperties": {
        ".+": {
          "type": "object",
          "properties": {
            "exampleKey1": {
              "type": "string",
              "maxLength": 10
            },
            "exampleKey2": {
              "type": "integer",
              "minimum": -100,
              "maximum": 100
            }
          },
          "required": [
            "exampleKey1",
            "exampleKey2"
          ]
        }
      }
    }
  },
  "required": [
    "exampleKey1",
    "exampleKey2",
    "exampleObject",
  ]
}